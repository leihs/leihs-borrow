#!/usr/bin/env bash
set -euo pipefail

PROJECT_DIR="$(cd -- "$(dirname "${BASH_SOURCE}")" ; cd ../.. > /dev/null 2>&1 && pwd -P)"
PROJECT_NAME=leihs-borrow
ASDF_PLUGIN=java
ASDF_PLUGIN_URL=https://github.com/halcyon/asdf-java.git

source $PROJECT_DIR/shared-clj/bin/env/asdf-helper.bash
asdf-update-plugin

# vi: ft=sh
